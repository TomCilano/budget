	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateService</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SelectService</servlet-name>
		<servlet-class>com.ironyard.servlet.SelectItemServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SelectService</servlet-name>
		<url-pattern>/select</url-pattern>
